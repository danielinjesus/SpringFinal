<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="com.gura.spring07.users.dto.UsersDto" alias="usersDto"/>
		<typeAlias type="com.gura.spring07.file.dto.FileDto" alias="fileDto"/>
		<typeAlias type="com.gura.spring07.cafe.dto.CafeDto" alias="cafeDto"/>
		<typeAlias type="com.gura.spring07.cafe.dto.CafeCommentDto" 
			alias="cafeCommentDto"/>
		<typeAlias type="com.gura.spring07.shop.dto.ShopDto" 
			alias="shopDto"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="com/gura/spring07/mybatis/UsersMapper.xml"/>
		<mapper resource="com/gura/spring07/mybatis/FileMapper.xml"/>
		<mapper resource="com/gura/spring07/mybatis/CafeMapper.xml"/>
		<mapper resource="com/gura/spring07/mybatis/CafeCommentMapper.xml"/>
		<mapper resource="com/gura/spring07/mybatis/ShopMapper.xml"/>
	</mappers>	
</configuration>











